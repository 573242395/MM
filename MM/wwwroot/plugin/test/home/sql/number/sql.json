			"field": "name",
			"name": "城市",
			"select": "`group`='市'"
		},
		"order": {
			"id": "id",
			"field": "name",
			"name": "订单状态",
			"value": "待售中,已预约,已售出,已下架,已删除"
		},
		"tid": {
			"type": "table",
			"table": "osl_number_taocan",
			"id": "tid",
			"field": "name",
			"name": "套餐"
		},
		"gid": {
			"type": "table",
			"id": "uid",
			"field": "nickName",
			"table": "mm_user",
			"name": "供应商",
			"select": "`gid` in (select x.gid FROM mm_user_group as x WHERE x.name like '%供应商%')"
		},
		"hide": {
			"type": "array",
			"id": "id",
			"field": "name",
			"name": "是否隐藏",
			"value": "否,是"
		}
	}
}